{
  "name": "@trong-orm/react",
  "version": "1.0.15",
  "author": "Sergey Popov",
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./query-builder": {
      "types": "./src/query-builder.ts",
      "import": "./src/query-builder.ts",
      "default": "./src/query-builder.ts"
    }
  },
  "typesVersions": {
    "*": {
      "query-builder": [
        "./src/query-builder.ts"
      ]
    }
  },
  "publishConfig": {
    "exports": {
      "./query-builder": {
        "types": "./src/query-builder.ts",
        "es2015": "./dist/query-builder.es.js",
        "require": "./dist/query-builder.umd.js",
        "import": "./dist/query-builder.es.js",
        "default": "./dist/query-builder.es.js"
      }
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./src/index.ts"
  },
  "scripts": {
    "compile": "yarn vite build",
    "lint": "eslint \"src/**/*.{tsx,ts}\"",
    "check-typing": "tsc --noEmit"
  },
  "dependencies": {
    "@trong-orm/absurd-web-backend": "^1.0.15",
    "@trong-orm/core": "^1.0.15",
    "@trong-orm/migrations-plugin": "^1.0.15",
    "@trong-orm/query-builder": "^1.0.15",
    "@trong-orm/react-queries-hooks": "^1.0.15",
    "@trong-orm/react-records-hooks": "^1.0.15",
    "@trong-orm/reactive-queries-plugin": "^1.0.15",
    "@trong-orm/records": "^1.0.15",
    "@trong-orm/sql": "^1.0.15"
  },
  "devDependencies": {
    "@trong-orm/common-scripts": "*"
  }
}