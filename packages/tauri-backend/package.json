{
  "name": "@trong-orm/tauri-backend",
  "version": "1.3.0",
  "author": "Sergey Popov",
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/tauri-backend/src/index.d.ts",
        "es2015": "./dist/index.es.js",
        "require": "./dist/index.umd.js",
        "import": "./dist/index.es.js",
        "default": "./dist/index.es.js"
      }
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/tauri-backend/src/index.d.ts"
  },
  "scripts": {
    "build": "yarn vite build",
    "lint": "eslint \"src/**/*.{tsx,ts}\" --max-warnings=0",
    "check-typing": "tsc --noEmit"
  },
  "dependencies": {
    "@trong-orm/core": "^1.3.0",
    "@trong-orm/sql": "^1.3.0",
    "@trong-orm/sql.js": "^1.6.7",
    "tauri-plugin-sqlite-api": "github:lzdyes/tauri-plugin-sqlite#v0.1.0"
  },
  "devDependencies": {
    "@trong-orm/common-scripts": "^1.3.0",
    "@types/emscripten": "^1.39.6",
    "ts-essentials": "^9.1.2"
  }
}
