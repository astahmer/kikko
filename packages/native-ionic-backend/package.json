{
  "name": "@trong-orm/native-ionic-backend",
  "version": "1.2.0",
  "author": "Sergey Popov",
  "license": "MIT",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/native-ionic-backend/src/index.d.ts",
        "es2015": "./dist/index.es.js",
        "require": "./dist/index.umd.js",
        "import": "./dist/index.es.js",
        "default": "./dist/index.es.js"
      }
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "./dist/native-ionic-backend/src/index.d.ts"
  },
  "scripts": {
    "compile": "yarn vite build",
    "lint": "eslint \"src/**/*.{tsx,ts}\"",
    "check-typing": "tsc --noEmit"
  },
  "dependencies": {
    "@trong-orm/core": "^1.2.0",
    "@trong-orm/sql": "^1.2.0",
    "@awesome-cordova-plugins/core": "^5.44.0",
    "@awesome-cordova-plugins/sqlite": "^5.44.0",
    "cordova-sqlite-storage": "^6.0.0"
  },
  "devDependencies": {
    "@trong-orm/common-scripts": "*"
  }
}